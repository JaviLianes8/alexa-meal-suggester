      "slots": [
        { "name": "mealType", "type": "MealType" }
      ]
    },
    {
      "name": "YesIntent",
      "samples": ["yes", "I like it", "sure"]
    },
    {
      "name": "NoIntent",
      "samples": ["no", "another one", "I don't like it"]
    },
    {
      "name": "AddToMealListIntent",
      "samples": ["add {dish} to {mealType}", "set {dish} as {mealType}"],
